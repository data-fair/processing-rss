{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "declaration": true,
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "#types": ["./types/index.ts"],
    },
  },
  "include": ["config","lib","test","types","index.ts"],
  "exclude": ["node_modules", "data",".type","config/*.cjs","eslint.config.mjs", "test"]
}
